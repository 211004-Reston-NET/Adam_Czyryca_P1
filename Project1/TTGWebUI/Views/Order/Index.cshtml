@{
}
@model IEnumerable<TTGWebUI.Models.OrderVM>
<h1>List of orders</h1>
<!--
<a class=" btn btn-primary text-white" asp-controller="Customer" asp-action="create">Add Customer</a>
-->
<br /> <!-- br os like\n -->
<br />
<table class=" table table-striped table-hover">
    <tr>
        <th>StoreFront</th>
        <th>Customer</th>
        <th>Total</th>
    </tr>

    @foreach (OrderVM order in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => order.StoreFront)</td>
            <td>@Html.DisplayFor(modelItem => order.Customer)</td>
            <td>@Html.DisplayFor(modelItem => order.Total)</td>
        </tr>
    }

</table>